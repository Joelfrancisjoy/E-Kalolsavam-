{
  "version": 2,
  "builds": [
    {
      "src": "backend/e_kalolsavam/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "backend/requirements.txt",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/backend/staticfiles/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/backend/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "backend/e_kalolsavam/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "e_kalolsavam.settings"
  }
}

